;
; Measurement Values map extensions
;
$$measurement-values-map-extension //= (
  &(spdm-indirect: 12) => spdm-indirect-map
)
spdm-indirect-map = {
  &(index: 0) => [ + uint ]
  * $$spdm-indirect-map-extension
}

;
; Flags map extensions
;
$$flags-map-extension //= (
  ? &(not-runtime-meas: 6) => bool
  ? &(not-immutable: 7) => bool
  ? &(not-tcb: 8) => bool
)
